// 将键绑定放入此文件中以覆盖默认值
[
    { "key": "ctrl+k ctrl+b",           "command": "workbench.action.toggleSidebarVisibility" },
    { "key": "ctrl+k ctrl+9",           "command": "editor.unfoldAll",
                                        "when": "editorFocus" },
    { "key": "alt+f3",                  "command": "editor.action.selectAllMatches",
                                        "when": "editorFocus && findWidgetVisible" },
    { "key": "ctrl+w",                  "command": "editor.action.smartSelect.grow",
                                        "when": "editorTextFocus" },
    { "key": "ctrl+f12",                "command": "workbench.action.gotoSymbol" },
    { "key": "ctrl+b",                  "command": "editor.action.goToDeclaration",
                                        "when": "editorTextFocus" },
    { "key": "ctrl+,",                  "command": "workbench.action.showAllSymbols" },
    // ctrl+. sometimes not working
    { "key": "alt+enter",                "command": "editor.action.quickFix",
                                        "when": "editorTextFocus" },
    { "key": "ctrl+[",                  "command": "editor.fold",
                                        "when": "editorTextFocus" },
    { "key": "ctrl+]",                  "command": "editor.unfold",
                                        "when": "editorTextFocus" },
    { "key": "ctrl+j",                "command": "editor.action.triggerSuggest" },
    { "key": "ctrl+q",                "command": "workbench.action.togglePanel" },
    { "key": "ctrl+1",                 "command": "workbench.action.openEditorAtIndex1" },
    { "key": "ctrl+2",                 "command": "workbench.action.openEditorAtIndex2" },
    { "key": "ctrl+3",                 "command": "workbench.action.openEditorAtIndex3" },
    { "key": "ctrl+4",                 "command": "workbench.action.openEditorAtIndex4" },
    { "key": "ctrl+5",                 "command": "workbench.action.openEditorAtIndex5" },
    { "key": "ctrl+6",                 "command": "workbench.action.openEditorAtIndex6" },
    { "key": "ctrl+7",                 "command": "workbench.action.openEditorAtIndex7" },
    { "key": "ctrl+8",                 "command": "workbench.action.openEditorAtIndex8" },
    { "key": "ctrl+9",                 "command": "workbench.action.openLastEditorInGroup" },
    { "key": "alt+1",                "command": "workbench.action.focusFirstEditorGroup" },
    { "key": "alt+2",                "command": "workbench.action.focusSecondEditorGroup" },
    { "key": "alt+3",                "command": "workbench.action.focusThirdEditorGroup" },
    { "key": "alt+4",                 "command": "" },
    { "key": "alt+5",                 "command": "" },
    { "key": "alt+6",                 "command": "" },
    { "key": "alt+7",                 "command": "" },
    { "key": "alt+8",                 "command": "" },
    { "key": "alt+9",                 "command": "" },
    { "key": "alt+0",                 "command": "" },



    { "key": "ctrl+d",                  "command": "editor.action.copyLinesDownAction",
                                       "when": "editorTextFocus" },
    { "key": "ctrl+alt+left",          "command": "workbench.action.navigateBack" },
    { "key": "ctrl+alt+right",          "command": "workbench.action.navigateForward" }
]